<Window x:Class="PhysioProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhysioProject"
        mc:Ignorable="d"
        WindowState="Maximized"
        WindowStyle="None"
        Title="MainWindow" Height="350" Width="525">
        
    <Grid >
        <StackPanel  Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Clients" MinWidth="100" Margin="10,0,0,0" Click="Button_Click_1"></Button>
            <Button Content="Products" MinWidth="100" Margin="10,0,0,0" Click="Button_Click_2" ></Button>
            <Button Content="Appointments" MinWidth="100" Margin="10,0,0,0" ></Button>
            <Button Content="Staff" MinWidth="100" Margin="10,0,0,0" ></Button>
            <Button Content="Payments" MinWidth="100" Margin="10,0,0,0" ></Button>
            <Button Content="Exit" MinWidth="100" Margin="10,0,0,0" Click="Button_Click" ></Button>




        </StackPanel>
        
        <Frame x:Name="Main" Margin="0,35,0,0" Navigated="Main_Navigated" NavigationUIVisibility="Hidden"/>



    </Grid>
</Window>
